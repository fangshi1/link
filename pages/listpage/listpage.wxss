.tabTit {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: solid 1px #ddd;
  position: fixed;
  top: 0;
  width: 750rpx;
  z-index: 1;
  background: #fff;
}

.tabTit .active {
  color: #70afe4;
  background: #fff;
}

.tabTit .active image {
  transform: rotate(180deg);
  background: none;
}

.tabTit image {
  width: 26rpx;
  height: 26rpx;
  vertical-align: middle;
  margin-left: 5px;
}

.tabLayer {
  box-shadow: 0 5px 5px rgba(0, 0, 0, 0.15);
  width: 750rpx;
  overflow: hidden;
  position: fixed;
  top: 104rpx;
  z-index: 1;
  background: #fff;
  /* padding-bottom: 26rpx; */
  border-bottom: solid 1px #eee;
  background: #fff; 
}
.tabLayer_tab{
  width: 90%;
